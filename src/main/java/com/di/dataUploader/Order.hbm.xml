<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.di.dataUploader.Order" table="orders">
		<meta attribute="class-description">

		</meta>
		<id name="id" column="id">
			<generator class="sequence">
				<param name="sequence">id_seq</param> 
			</generator>
		</id>
		<property name="orderDate" column="order_date" type="date" />
		<property name="status" column="status" type="string" />
		<property name="bookCode" column="book_Code" type="string" />
		<property name="customerCode" column="customer_code" type="string" />
	</class>
</hibernate-mapping>